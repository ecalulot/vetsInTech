import openpyxl
from openpyxl import Workbook

wb = Workbook()
ws = wb.active    this automatically actives the default worksheet

# ws2 = wb.create_chartsheet("new title")
ws3 = wb.create_sheet("new title")
ws.title = "add title"

wb.save("week3/spreadsheet/my_spreadsheet.xlsx")

