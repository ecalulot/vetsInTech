for row, post in enumerate(data, 1): 	# the 1 is optional and it tell it to start at 1 rather than 0 normally
    for column, post_value in enumerate(post.values(), 1):
        ws.cell(row=row, column=column, value=str(post_value))
wb.save("<my path to save the file>")




